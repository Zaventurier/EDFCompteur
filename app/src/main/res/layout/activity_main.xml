<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="408dp"
            android:layout_height="116dp" tools:layout_editor_absoluteY="1dp" tools:layout_editor_absoluteX="16dp">
        <ImageView
                android:layout_width="111dp"
                android:layout_height="104dp" app:srcCompat="@drawable/edf" android:id="@+id/imageView8"
        />
        <TextView
                android:text="EDF"
                android:textSize="50dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView7"
        />
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="195dp"
            android:layout_height="514dp" tools:layout_editor_absoluteY="117dp" tools:layout_editor_absoluteX="16dp">
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/identification"
                android:id="@+id/imageView9"
        />
        <TextView
                android:text="Identification"
                android:textAlignment="center"
                android:layout_width="131dp"
                android:layout_height="wrap_content" android:id="@+id/textView8"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/transfert" android:id="@+id/imageView10"
        />
        <TextView
                android:text="ImportDonnees"
                android:textAlignment="center"
                android:layout_width="129dp"
                android:layout_height="wrap_content" android:id="@+id/textView9"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="180dp"
            android:layout_height="515dp" tools:layout_editor_absoluteY="117dp" tools:layout_editor_absoluteX="214dp">
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/client" android:id="@+id/imageView11"
        />
        <TextView
                android:text="ReleveCompteur"
                android:textAlignment="center"
                android:layout_width="133dp"
                android:layout_height="wrap_content" android:id="@+id/textView10"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/sauvegarde" android:id="@+id/imageView12"
        />
        <TextView
                android:text="Sauvegarde"
                android:textAlignment="center"
                android:layout_width="128dp"
                android:layout_height="wrap_content" android:id="@+id/textView11"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>